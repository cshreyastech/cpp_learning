{
  "tasks": [
    {
      "type": "cppbuild",
      "label": "C/C++: g++-9 build active file",
      "command": "/usr/lib/ccache/g++-9",
      "args": [
        "-fdiagnostics-color=always",
        "-g",
        "${file}",        
        
        "-I${workspaceFolder}/external",
        "-I${workspaceFolder}/external/asio-1.28.1/include",
        "-I${workspaceFolder}/02-MMO/NetClient/include",
        "-I${workspaceFolder}/03-olcPGEX/MMO_Server/include",
        "-I${workspaceFolder}/04-ROS_ML/ROS_ML_Server/include",
        "-I${workspaceFolder}/05-ros_ml/ros_ml_server/include",
        "-I${workspaceFolder}/05-ros_ml/ros_ml_client/include",

        "-o",
        "${fileDirname}/build/${fileBasenameNoExtension}",
        "-lglfw",
        "-lGLU",
        "-lGL",
        "-lGLEW",
        "-lpthread",
        "-glfw",
      ],
      "options": {
        "cwd": "${fileDirname}"
      },
      "problemMatcher": [
        "$gcc"
      ],
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "detail": "Task generated by Debugger."
    }
  ],
  "version": "2.0.0"
}