{
  "tasks": [
    {
      "type": "cppbuild",
      "label": "C/C++: g++-9 build active file",
      "command": "/usr/lib/ccache/g++-9",
      "args": [
        "-fdiagnostics-color=always",
        "-g",
        "${file}",
        "-pthread",
        "-I${workspaceRoot}/external/asio-1.28.1/include",
        "-I${workspaceRoot}/02-MMO/NetCommon/include",
        "-I${workspaceRoot}/03-olcPGEX/MMO_Server/include",
        "-I${workspaceRoot}/04-ROS_ML/ROS_ML_Server/include",
        "-o",
        "${fileDirname}/build/${fileBasenameNoExtension}",
        "-lglfw",
        "-lGLU",
        "-lGL",
        "-lGLEW",
        "-lpthread"
      ],
      "options": {
        "cwd": "${fileDirname}"
      },
      "problemMatcher": [
        "$gcc"
      ],
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "detail": "Task generated by Debugger."
    }
  ],
  "version": "2.0.0"
}